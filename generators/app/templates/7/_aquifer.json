{
  "name": "<%= humanName %>",
  "core": 7,
  "build": {
    "method": "drush make",
    "directory": "build",
    "makeFile": "drupal.make.yml"
  },
  "sync": {
    "root": {
      "destination": "",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "required": true,
      "contents": true
    },
    "modules/custom": {
      "destination": "sites/all/modules/custom",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "required": true
    },
    "modules/features": {
      "destination": "sites/all/modules/features",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "required": true
    },
    "themes/custom": {
      "destination": "sites/all/themes/custom",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "required": true
    },
    "profiles/<%= appName %>": {
      "destination": "profiles/<%= appName %>",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "required": true
    },
    "settings": {
      "destination": "sites/default",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "file",
      "hook": "postBuild",
      "required": true,
      "contents": true
    },
    "files": {
      "destination": "sites/default/files",
      "method": "symlink",
      "conflict": "overwrite",
      "type": "dir",
      "hook": "postBuild",
      "contents": true
    }
  },
  "run": {
    "scripts": {
      "refresh": [
        "drush cc drush",
        "drush en -y master",
        "drush master-execute -y --scope=local",
        "drush updb -y",
        "drush cc all",
        "drush fra -y"
      ]
    },
    "preSync": [],
    "postBuild": []
  },
  "extensions": {
    "aquifer-git": {
      "source": "aquifer-git",
      "remote": "",
      "branch": "master",
      "name": "CI Bot",
      "email": "ci@estudiomanati.com",
      "deploymentFiles": [
        {
          "src": "root/.gitignore",
          "dest": ".gitignore"
        },
        {
          "src": "root/.htaccess",
          "dest": ".htaccess"
        }
      ],
      "excludeLinks": ["sites/default/files"],
      "addlinks": [],
      "delPatterns": ["*", "!.git"]
    }
  },
  "environments": {}
}
